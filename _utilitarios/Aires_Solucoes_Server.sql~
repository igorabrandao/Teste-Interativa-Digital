  `STATUS_DEVOLUCAO` INT NOT NULL COMMENT '(TROCA DE CHIP/TROCA DE APARELHO/DEVOLUÇÃO COMPLETA) \n\n--TROCA DE CHIP/APARELHO = TERMO DE SUBSTITUIÇÃO // TERMO DE ENTREGA = DEVOLUÇÃO COMPLETA',
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
