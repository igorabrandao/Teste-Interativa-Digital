SET @OLD_UNIQUE_CHECKS=@@UNIQUE_CHECKS, UNIQUE_CHECKS=0;
SET @OLD_FOREIGN_KEY_CHECKS=@@FOREIGN_KEY_CHECKS, FOREIGN_KEY_CHECKS=0;
SET @OLD_SQL_MODE=@@SQL_MODE, SQL_MODE='TRADITIONAL,ALLOW_INVALID_DATES';

DROP SCHEMA IF EXISTS `Aires_Solucoes` ;
CREATE SCHEMA IF NOT EXISTS `Aires_Solucoes` DEFAULT CHARACTER SET utf8 COLLATE utf8_general_ci ;
USE `Aires_Solucoes` ;

-- -----------------------------------------------------
-- Table `Aires_Solucoes`.`Config.Indice`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Config.Indice` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Config.Indice` (
  `ID_INDICE` INT NOT NULL AUTO_INCREMENT,
  `NOME_INDICE` VARCHAR(30) NULL,
  `VALOR_PERCENTUAL` FLOAT NULL,
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID_INDICE`),
  UNIQUE INDEX `NOME_INDICE_UNIQUE` (`NOME_INDICE` ASC))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Aires_Solucoes`.`Cliente.Empresa`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Cliente.Empresa` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Cliente.Empresa` (
  `ID_CLIENTE` INT NOT NULL AUTO_INCREMENT,
  `TIPO_CLIENTE` INT NOT NULL COMMENT '--1 = BASE\n--2 = PROSPECÇÃO',
  `MODALIDADE_EMPRESA` INT NOT NULL COMMENT '--1 = MATRIZ\n--2 = FILIAL',
  `CNPJ` VARCHAR(18) NOT NULL,
  `INSCRICAO_ESTADUAL` VARCHAR(15) NULL,
  `RAZAO_SOCIAL` VARCHAR(100) NOT NULL,
  `NOME_FANTASIA` VARCHAR(50) NOT NULL,
  `DATA_ABERTURA` VARCHAR(10) NULL COMMENT 'Registra a data de abertura da empresa',
  `N_FUNCIONARIOS` VARCHAR(20) NULL,
  `PORTE` INT NOT NULL COMMENT '--Microempresa\n--Pequena empresa\n--Média empresa\n--Média-grande empresa\n--Grande empresa',
  `ENDERECO` VARCHAR(100) NULL,
  `NUMERO` INT NULL,
  `CEP` VARCHAR(9) NULL,
  `BAIRRO` VARCHAR(50) NULL,
  `CIDADE` VARCHAR(50) NULL,
  `UF` CHAR(2) NULL,
  `TELEFONE_EMPRESA` VARCHAR(15) NULL,
  `TELEFONE` VARCHAR(15) NULL,
  `TELEFONE2` VARCHAR(15) NULL,
  `RAMAL` VARCHAR(10) NULL,
  `RAMAL2` VARCHAR(10) NULL,
  `CELULAR` VARCHAR(15) NULL,
  `CELULAR2` VARCHAR(15) NULL,
  `EMAIL` VARCHAR(100) NOT NULL,
  `EMAIL2` VARCHAR(100) NULL,
  `NOME_CONTATO` VARCHAR(50) NOT NULL,
  `NOME_CONTATO2` VARCHAR(50) NULL,
  `DATA_FECHA` VARCHAR(20) NULL,
  PRIMARY KEY (`ID_CLIENTE`),
  UNIQUE INDEX `CNPJ_UNIQUE` (`CNPJ` ASC),
  UNIQUE INDEX `INSCRICAO_ESTADUAL_UNIQUE` (`INSCRICAO_ESTADUAL` ASC))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Aires_Solucoes`.`Cliente.Gestor`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Cliente.Gestor` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Cliente.Gestor` (
  `ID_GESTOR` INT NOT NULL AUTO_INCREMENT,
  `ID_EMPRESA` INT NOT NULL,
  `CPF` VARCHAR(15) NOT NULL,
  `NOME_COMPLETO` VARCHAR(100) NOT NULL,
  `DATA_NASCIMENTO` VARCHAR(10) NULL,
  `RG` VARCHAR(15) NULL,
  `TELEFONE` VARCHAR(15) NULL,
  `EMAIL` VARCHAR(100) NOT NULL,
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID_GESTOR`),
  UNIQUE INDEX `CPF_UNIQUE` (`CPF` ASC),
  UNIQUE INDEX `RG_UNIQUE` (`RG` ASC),
  INDEX `ID_GESTOR_EMPRESA_FK_idx` (`ID_EMPRESA` ASC),
  CONSTRAINT `ID_GESTOR_EMPRESA_FK`
    FOREIGN KEY (`ID_EMPRESA`)
    REFERENCES `Aires_Solucoes`.`Cliente.Empresa` (`ID_CLIENTE`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Aires_Solucoes`.`Cliente.AuxEmpresaRepresentante`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Cliente.AuxEmpresaRepresentante` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Cliente.AuxEmpresaRepresentante` (
  `ID_EMPRESA_REPRESENTANTE` INT NOT NULL AUTO_INCREMENT,
  `ID_EMPRESA` INT NOT NULL,
  `ID_REPRESENTANTE` INT NOT NULL,
  `DATA_FECHA` VARCHAR(20) NULL,
  PRIMARY KEY (`ID_EMPRESA_REPRESENTANTE`),
  INDEX `AUX_REPRESENTANTE_ID_idx` (`ID_EMPRESA` ASC),
  INDEX `AUX_REPRESENTANTE_CPF_idx` (`ID_REPRESENTANTE` ASC),
  CONSTRAINT `AUX_EMPRESA_ID_FK`
    FOREIGN KEY (`ID_EMPRESA`)
    REFERENCES `Aires_Solucoes`.`Cliente.Empresa` (`ID_CLIENTE`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `AUX_REPRESENTANTE_ID_FK`
    FOREIGN KEY (`ID_REPRESENTANTE`)
    REFERENCES `Aires_Solucoes`.`Cliente.Gestor` (`ID_GESTOR`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Aires_Solucoes`.`Aires.Contrato`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Aires.Contrato` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Aires.Contrato` (
  `ID_CONTRATO` INT NOT NULL AUTO_INCREMENT,
  `ID_EMPRESA` INT NOT NULL,
  `DATA_INICIO_CONTRATO` VARCHAR(20) NOT NULL,
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID_CONTRATO`),
  CONSTRAINT `CONTRATO_AIRES_ID_EMPRESA_FK`
    FOREIGN KEY (`ID_EMPRESA`)
    REFERENCES `Aires_Solucoes`.`Cliente.Empresa` (`ID_CLIENTE`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Aires_Solucoes`.`Operadora.Operadora`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Operadora.Operadora` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Operadora.Operadora` (
  `ID_OPERADORA` INT NOT NULL AUTO_INCREMENT,
  `NOME_OPERADORA` VARCHAR(50) NOT NULL,
  `ORDEM` INT NOT NULL COMMENT 'POSIÇÃO EM QUE A OPERADORA APARECERÁ',
  `CSP` INT NULL,
  `CONTATO_FIXO` VARCHAR(15) NULL,
  `CONTATO_MOVEL` VARCHAR(15) NULL,
  `CONTATO_LINK_DEDICADO` VARCHAR(15) NULL,
  `CONTATO_DADOS` VARCHAR(15) NULL,
  `CONTATO_DDR` VARCHAR(15) NULL,
  `CONTATO_OUVIDORIA` VARCHAR(15) NULL,
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID_OPERADORA`),
  UNIQUE INDEX `RAZAO_SOCIAL_UNIQUE` (`NOME_OPERADORA` ASC))
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Aires_Solucoes`.`Aires.AuxContrato`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Aires.AuxContrato` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Aires.AuxContrato` (
  `ID_AUX_CONTRATO` INT NOT NULL AUTO_INCREMENT,
  `ID_CONTRATO` INT NOT NULL,
  `OPERADORA` VARCHAR(20) NOT NULL COMMENT '--UTILIZA O MODELO BINÁRIO PARA DETERMINAR QUAIS OPERADORAS FAZEM PARTE\n\nEX: VIVO (SIM), CLARO (NÃO), OI (SIM), TIM (SIM), NEXTEL (NÃO) = 10110',
  `PERCENTUAL_NEGOCIACAO` VARCHAR(30) NOT NULL,
  `GASTO_MENSAL_ESTIMADO` VARCHAR(30) NOT NULL,
  `REDUCAO_MENSAL_ESTIMADA` VARCHAR(30) NOT NULL,
  `REDUCAO_MENSAL_ESTIMADA_PERCENTUAL` VARCHAR(30) NOT NULL,
  `REDUCAO_ANUAL_ESTIMADA` VARCHAR(30) NOT NULL,
  `REDUCAO_ANUAL_ESTIMADA_PERCENTUAL` VARCHAR(30) NOT NULL,
  `GASTO_MENSAL_FINAL` VARCHAR(30) NULL,
  `REDUCAO_MENSAL_FINAL` VARCHAR(30) NULL,
  `REDUCAO_MENSAL_PERCENTUAL_FINAL` VARCHAR(30) NULL,
  `REDUCAO_ANUAL_FINAL` VARCHAR(30) NULL,
  `REDUCAO_ANUAL_PERCENTUAL_FINAL` VARCHAR(30) NULL,
  `VALOR_LINHA_ATIVA` VARCHAR(30) NOT NULL,
  `QTD_LINHA_ATIVA` INT NOT NULL,
  `VALOR_TOTAL_LINHA_ATIVA` VARCHAR(30) NOT NULL,
  `VALOR_LINHA_INATIVA` VARCHAR(30) NULL,
  `QTD_LINHA_INATIVA` INT NULL,
  `VALOR_TOTAL_LINHA_INATIVA` VARCHAR(30) NULL,
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID_AUX_CONTRATO`),
  INDEX `CONTRATO_idx` (`ID_CONTRATO` ASC),
  CONSTRAINT `AUX_CONTRATO_FK`
    FOREIGN KEY (`ID_CONTRATO`)
    REFERENCES `Aires_Solucoes`.`Aires.Contrato` (`ID_CONTRATO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


