-- -----------------------------------------------------
-- Table `Aires_Solucoes`.`Operadora.Contrato`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Operadora.Contrato` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Operadora.Contrato` (
  `ID_CONTRATO_OPERADORA` INT NOT NULL AUTO_INCREMENT,
  `N_CONTA` VARCHAR(20) NULL,
  `ID_TIPO_CONTRATO` INT NOT NULL COMMENT 'OPÇÕES\n-NOVO\n-INCREMENTO\n-RENOVAÇÃO\n-MIGRAÇÃO DE PLANO\n-TRANSFERÊNCIA DE TITULARIDADE',
  `ID_EMPRESA` INT NOT NULL,
  `ID_TIPO_SERVICO` INT NOT NULL,
  `ID_OPERADORA` INT NOT NULL,
  `DATA_ASSINATURA` VARCHAR(20) NOT NULL,
  `DATA_ATIVACAO` VARCHAR(20) NULL,
  `CARENCIA` VARCHAR(20) NOT NULL,
  `VALOR_TOTAL_CONTRATO` VARCHAR(20) NULL,
  `DATA_FECHA` VARCHAR(20) NULL,
  PRIMARY KEY (`ID_CONTRATO_OPERADORA`),
  INDEX `CONTRATO_ID_EMPRESA_idx` (`ID_EMPRESA` ASC),
  INDEX `TIPO_CONTRATO_idx` (`ID_TIPO_CONTRATO` ASC),
  INDEX `TIPO_SERVICO_CONTRATO_OPERADORA_idx` (`ID_TIPO_SERVICO` ASC),
  INDEX `ID_OPERADORA_CONTRATO_FK_idx` (`ID_OPERADORA` ASC),
  CONSTRAINT `ID_TIPO_CONTRATO_OPERADORA_FK`
    FOREIGN KEY (`ID_TIPO_CONTRATO`)
    REFERENCES `Aires_Solucoes`.`Operadora.TipoContrato` (`ID_TIPO_CONTRATO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_CONTRATO_OPERADORA_ID_EMPRESA_FK`
    FOREIGN KEY (`ID_EMPRESA`)
    REFERENCES `Aires_Solucoes`.`Cliente.Empresa` (`ID_CLIENTE`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_TIPO_SERVICO_CONTRATO_OPERADORA_FK`
    FOREIGN KEY (`ID_TIPO_SERVICO`)
    REFERENCES `Aires_Solucoes`.`Operadora.ContratoTipoServico` (`ID_CONTRATO_TIPO_SERVICO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_OPERADORA_CONTRATO_FK`
    FOREIGN KEY (`ID_OPERADORA`)
    REFERENCES `Aires_Solucoes`.`Operadora.Operadora` (`ID_OPERADORA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_EMPRESA_CONTRATO_OPERADORA_FK`
    FOREIGN KEY (`ID_EMPRESA`)
    REFERENCES `Aires_Solucoes`.`Cliente.Empresa` (`ID_CLIENTE`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


