-- Table `Aires_Solucoes`.`Book.Tarifa`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Book.Tarifa` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Book.Tarifa` (
  `ID_TARIFA` INT NOT NULL AUTO_INCREMENT,
  `ID_PLATAFORMA_LD` INT NOT NULL COMMENT '--OPÇÕES\n-VC1 MÓVEL\n-VC1 FIXO\n-VC1 INTRA-REDE\n\n-VC2 MÓVEL TARIFA REDUZIDA\n-VC2 MÓVEL TARIFA NORMAL\n-VC2 FIXO TARIFA REDUZIDA\n-VC2 FIXO TARIFA NORMAL\n-VC2 INTRA-REDE TARIFA REDUZIDA\n-VC2 INTRA-REDE TARIFA NORMAL\n\n-VC3 MÓVEL TARIFA REDUZIDA\n-VC3 MÓVEL TARIF /* comment truncated */ /*A NORMAL
-VC3 FIXO TARIFA REDUZIDA
-VC3 FIXO TARIFA NORMAL
-VC3 INTRA-REDE TARIFA REDUZIDA
-VC3 INTRA-REDE TARIFA NORMAL

-DSL1 TARIFA REDUZIDA
-DSL1 TARIFA NORMAL

-DSL2 TARIFA REDUZIDA
-DSL2 TARIFA NORMAL

-AD TARIFA REDUZIDAL
-AD TARIFA NORMAL*/',
  `ID_TIPO_TARIFA` INT NULL,
  `ID_SUBTIPO_TARIFA` INT NULL,
  `VOLUME` VARCHAR(20) NULL,
  `TIPO_COMPARTILHAMENTO` VARCHAR(20) NULL,
  `TARIFA_UNICA` VARCHAR(15) NULL,
  `TARIFA_EXCEDENTE` VARCHAR(15) NULL,
  `TARIFA_AC` VARCHAR(50) NULL,
  `TARIFA_AP` VARCHAR(50) NULL,
  `TARIFA_AM` VARCHAR(50) NULL,
  `TARIFA_BA` VARCHAR(50) NULL,
  `TARIFA_AL` VARCHAR(50) NULL,
  `TARIFA_CE` VARCHAR(50) NULL,
  `TARIFA_DF` VARCHAR(50) NULL,
  `TARIFA_ES` VARCHAR(50) NULL,
  `TARIFA_GO` VARCHAR(50) NULL,
  `TARIFA_MA` VARCHAR(50) NULL,
  `TARIFA_MS` VARCHAR(50) NULL,
  `TARIFA_MT` VARCHAR(50) NULL,
  `TARIFA_MG` VARCHAR(50) NULL,
  `TARIFA_PA` VARCHAR(50) NULL,
  `TARIFA_PB` VARCHAR(50) NULL,
  `TARIFA_PR` VARCHAR(50) NULL,
  `TARIFA_PE` VARCHAR(50) NULL,
  `TARIFA_PI` VARCHAR(50) NULL,
  `TARIFA_RJ` VARCHAR(50) NULL,
  `TARIFA_RN` VARCHAR(50) NULL,
  `TARIFA_RS` VARCHAR(50) NULL,
  `TARIFA_RO` VARCHAR(50) NULL,
  `TARIFA_RR` VARCHAR(50) NULL,
  `TARIFA_SC` VARCHAR(50) NULL,
  `TARIFA_SP` VARCHAR(50) NULL,
  `TARIFA_SE` VARCHAR(50) NULL,
  `TARIFA_TO` VARCHAR(50) NULL,
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID_TARIFA`),
  INDEX `TIPO_TARIFA_idx` (`ID_TIPO_TARIFA` ASC),
  INDEX `PLATAFORMA_LD_idx` (`ID_PLATAFORMA_LD` ASC),
  INDEX `SUBTIPO_TARIFA_idx` (`ID_SUBTIPO_TARIFA` ASC),
  CONSTRAINT `TIPO_TARIFA_FK`
    FOREIGN KEY (`ID_TIPO_TARIFA`)
    REFERENCES `Aires_Solucoes`.`Book.TipoTarifa` (`ID_TIPO_TARIFA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `SUBTIPO_TARIFA_FK`
    FOREIGN KEY (`ID_SUBTIPO_TARIFA`)
    REFERENCES `Aires_Solucoes`.`Book.SubtipoTarifa` (`ID_SUBTIPO_TARIFA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `PLATAFORMA_LD_FK`
    FOREIGN KEY (`ID_PLATAFORMA_LD`)
    REFERENCES `Aires_Solucoes`.`Book.PlataformaLD` (`ID_PLATAFORMA_LD`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


