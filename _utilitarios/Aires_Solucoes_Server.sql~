  `NOME_ATENDENTE` VARCHAR(50) NULL,
  `DATA_HORARIO_ATENDIMENTO` VARCHAR(20) NULL,
  `DATA_ABERTURA_PROTOCOLO` VARCHAR(20) NULL,
  `DATA_RESOLUCAO_PREVISTA` VARCHAR(20) NULL,
  `HISTORICO` VARCHAR(1000) NULL,
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID_ATENDIMENTO`),
  INDEX `ATENDIMENTO_ID_OPERADORA_idx` (`ID_OPERADORA` ASC),
  INDEX `ATENDIMENTO_TIPO_SERVICO_idx` (`TIPO_SERVICO` ASC),
  CONSTRAINT `ATENDIMENTO_ID_OPERADORA_FK`
    FOREIGN KEY (`ID_OPERADORA`)
