    FOREIGN KEY (`TIPO_SERVICO`)
    REFERENCES `Aires_Solucoes`.`Atendimento.TipoServico` (`ID_TIPO_SERVICO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


