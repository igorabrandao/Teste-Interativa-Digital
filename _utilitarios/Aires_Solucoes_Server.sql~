CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Operadora.PlanoContrato` (
  `ID_PLANO_CONTRATO` INT NOT NULL AUTO_INCREMENT,
  `ID_CONTRATO_OPERADORA` INT NOT NULL,
  `ID_TIPO_PLANO` INT NOT NULL,
  `ID_PLANO_ASSOCIADO` INT NULL,
  `DESCRITIVO_PLANO` VARCHAR(100) NOT NULL,
  `QUANTIDADE_PLANO` INT NOT NULL,
  `VALOR_ASSINATURA_PLANO` VARCHAR(15) NOT NULL,
  `DESCRITIVO_PACOTE_VOZ` VARCHAR(100) NULL,
  `TARIFA_LOCAL` VARCHAR(15) NULL,
  `VOLUME_PLANO` VARCHAR(20) NULL,
  `MINUTOS` VARCHAR(10) NULL,
  `VALOR_PAC_MIN` VARCHAR(15) NULL,
  `DESCONTO_PLANO_12` VARCHAR(15) NULL,
  `DESCONTO_PLANO_24` VARCHAR(15) NULL,
  `VALOR_TOTAL_PLANO` VARCHAR(15) NULL,
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID_PLANO_CONTRATO`),
  INDEX `PLANO_CONTRATO_idx` (`ID_CONTRATO_OPERADORA` ASC),
  INDEX `TIPO_PLANO_CONTRATO_FK_idx` (`ID_TIPO_PLANO` ASC),
  INDEX `PLANO_ASSOCIADO_FK_idx` (`ID_PLANO_ASSOCIADO` ASC),
  CONSTRAINT `PLANO_CONTRATO_FK`
    FOREIGN KEY (`ID_CONTRATO_OPERADORA`)
    REFERENCES `Aires_Solucoes`.`Operadora.Contrato` (`ID_CONTRATO_OPERADORA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `TIPO_PLANO_CONTRATO_FK`
    FOREIGN KEY (`ID_TIPO_PLANO`)
    REFERENCES `Aires_Solucoes`.`Operadora.TipoPlano` (`ID_TIPO_PLANO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `PLANO_ASSOCIADO_FK`
    FOREIGN KEY (`ID_PLANO_ASSOCIADO`)
    REFERENCES `Aires_Solucoes`.`Operadora.PlanoContrato` (`ID_PLANO_CONTRATO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


