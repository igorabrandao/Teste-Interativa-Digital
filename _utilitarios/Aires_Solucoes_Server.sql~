-- Table `Aires_Solucoes`.`Patrimonio.LoteAparelho`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Patrimonio.LoteAparelho` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Patrimonio.LoteAparelho` (
  `ID_LOTE_PATRIMONIO` INT NOT NULL AUTO_INCREMENT,
  `ID_CONTRATO_OPERADORA` INT NOT NULL,
  `DATA_FECHA` VARCHAR(20) NULL,
  PRIMARY KEY (`ID_LOTE_PATRIMONIO`),
  INDEX `ID_CONTRATO_OPERADORA_LOTE_idx` (`ID_CONTRATO_OPERADORA` ASC),
  CONSTRAINT `ID_CONTRATO_OPERADORA_LOTE_FK`
    FOREIGN KEY (`ID_CONTRATO_OPERADORA`)
    REFERENCES `Aires_Solucoes`.`Operadora.Contrato` (`ID_CONTRATO_OPERADORA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `Aires_Solucoes`.`Patrimonio.Patrimonio`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`Patrimonio.Patrimonio` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`Patrimonio.Patrimonio` (
  `ID_PATRIMONIO` INT NOT NULL AUTO_INCREMENT,
  `IMEI` VARCHAR(20) NOT NULL,
  `ID_LOTE_APARELHO` INT NOT NULL,
  `ID_MARCA` INT NOT NULL,
  `MODELO` VARCHAR(50) NULL,
  `ID_TIPO_ATIVO` INT NOT NULL,
  `ID_FORMA_AQUISICAO` INT NOT NULL,
  `DATA_COLETA` VARCHAR(25) NULL,
  `STATUS_APARELHO` INT NOT NULL,
  `COMPROVANTE_COLETA` VARCHAR(255) NULL,
  `DATA_FECHA` VARCHAR(20) NULL,
  PRIMARY KEY (`ID_PATRIMONIO`),
  UNIQUE INDEX `IMEI_UNIQUE` (`IMEI` ASC),
  INDEX `PATRIMONIO_MARCA_idx` (`ID_MARCA` ASC),
  INDEX `PATRIMONIO_FORMA_AQUISICAO_idx` (`ID_FORMA_AQUISICAO` ASC),
  INDEX `PATRIMONIO_TIPO_ATIVO_idx` (`ID_TIPO_ATIVO` ASC),
  INDEX `PATRIMONIO_LOTE_APARELHO_idx` (`ID_LOTE_APARELHO` ASC),
  CONSTRAINT `PATRIMONIO_MARCA`
    FOREIGN KEY (`ID_MARCA`)
    REFERENCES `Aires_Solucoes`.`Patrimonio.Marca` (`ID_MARCA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `PATRIMONIO_FORMA_AQUISICAO`
    FOREIGN KEY (`ID_FORMA_AQUISICAO`)
    REFERENCES `Aires_Solucoes`.`Patrimonio.FormaAquisicao` (`ID_FORMA_AQUISICAO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `PATRIMONIO_TIPO_ATIVO`
    FOREIGN KEY (`ID_TIPO_ATIVO`)
    REFERENCES `Aires_Solucoes`.`Patrimonio.TipoAtivo` (`ID_TIPO_ATIVO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `PATRIMONIO_LOTE_APARELHO`
    FOREIGN KEY (`ID_LOTE_APARELHO`)
    REFERENCES `Aires_Solucoes`.`Patrimonio.LoteAparelho` (`ID_LOTE_PATRIMONIO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


