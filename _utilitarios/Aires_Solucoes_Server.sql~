-- -----------------------------------------------------
-- Table `Aires_Solucoes`.`PEC.Det`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `Aires_Solucoes`.`PEC.Det` ;

CREATE TABLE IF NOT EXISTS `Aires_Solucoes`.`PEC.Det` (
  `ID_PEC_DET` INT NOT NULL AUTO_INCREMENT,
  `ID_PEC` INT NOT NULL,
  `ID_TIPO_DET` INT NOT NULL,
  `ID_LINHA` INT NOT NULL,
  `ID_TIPO_UTILIZACAO` INT NOT NULL,
  `ID_TIPO_LIGACAO` INT NOT NULL,
  `ID_TIPO_CHAMADA` INT NOT NULL,
  `DESCRITIVO` VARCHAR(50) NULL,
  `INCLUSO` VARCHAR(20) NULL,
  `UTILIZADO` VARCHAR(20) NULL,
  `EXCEDENTE` VARCHAR(20) NULL,
  `CSP` VARCHAR(4) NULL,
  `DATA` VARCHAR(20) NULL,
  `HORA` VARCHAR(10) NULL,
  `ORIGEM` VARCHAR(50) NULL,
  `DESTINO` VARCHAR(50) NULL,
  `ORIGEM_DESTINO` VARCHAR(100) NULL,
  `PAIS_OPERADORA` VARCHAR(100) NULL,
  `SERVICO` VARCHAR(50) NULL,
  `N_CHAMADO` VARCHAR(20) NULL,
  `TARIFA` VARCHAR(10) NULL,
  `DURACAO` VARCHAR(10) NULL,
  `TARIFADO` VARCHAR(20) NULL,
  `TIPO` VARCHAR(30) NULL,
  `QUANTIDADE` VARCHAR(30) NULL,
  `VALOR` VARCHAR(20) NULL,
  `VALOR_COBRADO` VARCHAR(20) NULL,
  `INTERCONEXAO` VARCHAR(20) NULL,
  `TIPO_INFO_DET` INT NULL COMMENT 'DEFINE THE TYPE OF INFORMATION\n--1 => SUBTOTAL\n--2 => DETALHAMENTO',
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID_PEC_DET`),
  INDEX `ID_PEC_DET_VOZ_LINHA_FK_idx` (`ID_LINHA` ASC),
  INDEX `ID_PEC_DET_VOZ_FK_idx` (`ID_PEC` ASC),
  INDEX `ID_PEC_TIPO_UTILIZACAO_FK_idx` (`ID_TIPO_UTILIZACAO` ASC),
  INDEX `ID_PEC_TIPO_LIGACAO_FK_idx` (`ID_TIPO_LIGACAO` ASC),
  INDEX `ID_PEC_TIPO_CHAMADA_FK_idx` (`ID_TIPO_CHAMADA` ASC),
  INDEX `ID_PEC_TIPO_DET_FK_idx` (`ID_TIPO_DET` ASC),
  CONSTRAINT `ID_PEC_DET_VOZ_LINHA_FK`
    FOREIGN KEY (`ID_LINHA`)
    REFERENCES `Aires_Solucoes`.`PEC.Linhas` (`ID_PEC_LINHA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_PEC_DET_VOZ_FK`
    FOREIGN KEY (`ID_PEC`)
    REFERENCES `Aires_Solucoes`.`PEC.Pec` (`ID_PEC`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_PEC_TIPO_UTILIZACAO_FK`
    FOREIGN KEY (`ID_TIPO_UTILIZACAO`)
    REFERENCES `Aires_Solucoes`.`PEC.DetTipoUtilizacao` (`ID_PEC_TIPO_UTILIZACAO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_PEC_TIPO_LIGACAO_FK`
    FOREIGN KEY (`ID_TIPO_LIGACAO`)
    REFERENCES `Aires_Solucoes`.`PEC.DetTipoLigacao` (`ID_PEC_TIPO_LIGACAO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_PEC_TIPO_CHAMADA_FK`
    FOREIGN KEY (`ID_TIPO_CHAMADA`)
    REFERENCES `Aires_Solucoes`.`PEC.DetTipoChamada` (`ID_PEC_TIPO_CHAMADA`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_PEC_TIPO_DET_FK`
    FOREIGN KEY (`ID_TIPO_DET`)
    REFERENCES `Aires_Solucoes`.`PEC.TipoDet` (`ID_TIPO_DET`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


