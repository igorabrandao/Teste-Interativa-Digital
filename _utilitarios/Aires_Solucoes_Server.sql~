  `LOCAL` VARCHAR(30) NULL,
  `NOTA_FISCAL` VARCHAR(20) NULL,
  `PARCELA` VARCHAR(10) NULL,
  `VALOR_DESCONTO` VARCHAR(20) NULL,
  `VALOR_TOTAL` VARCHAR(20) NULL,
  `VALOR_PARCELA` VARCHAR(20) NULL,
  `VALOR` VARCHAR(20) NULL,
  `DATA_FECHA` VARCHAR(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID_OUTROS_LANCAMENTOS_DET`),
  INDEX `ID_PEC_DET_VOZ_FK_idx` (`ID_PEC` ASC),
  INDEX `ID_PEC_TIPO_DET_OUTRO_LANCAMENTO_FK_idx` (`ID_TIPO_LANCAMENTO` ASC),
  INDEX `ID_OUTRO_LANCAMENTO_DET_LANCAMENTO_FK_idx` (`ID_LANCAMENTO` ASC),
  CONSTRAINT `ID_PEC_DET_OUTRO_LANCAMENTO_FK`
    FOREIGN KEY (`ID_PEC`)
    REFERENCES `Aires_Solucoes`.`PEC.Pec` (`ID_PEC`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_PEC_TIPO_DET_OUTRO_LANCAMENTO_FK`
    FOREIGN KEY (`ID_TIPO_LANCAMENTO`)
    REFERENCES `Aires_Solucoes`.`PEC.TipoLancamento` (`ID_TIPO_LANCAMENTO`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `ID_OUTRO_LANCAMENTO_DET_LANCAMENTO_FK`
    FOREIGN KEY (`ID_LANCAMENTO`)
    REFERENCES `Aires_Solucoes`.`PEC.OutrosLancamentos` (`ID_PEC_OUTRO_LANCAMENTO`)
